# -*- shell-script -*-
# Sets PYVERSIONS to be pyenv versions that
# we can use in the master branch.
if [[ $0 == ${BASH_SOURCE[0]} ]] ; then
    typeset -p PYVERSIONS
    echo "Note: this script should be *sourced* rather than run directly through bash"
    exit 1
fi

#E export PYVERSIONS='3.6.15 3.7.16 pypy3.6-7.3.1 3.7.16 pypy3.7-7.3.9 pypy3.8-7.3.10 pypy3.9-7.3.10 pyston-2.3.5 3.8.16 3.9.16 3.10.9'
export PYVERSIONS='3.6.15 3.7.17 3.8.17 3.9.17'
